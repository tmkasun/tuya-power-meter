{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "outDir": "dist",
  },
  "include": ["./**/*"],
  "exclude": [
    "web-ui/**/*",
    "node_modules/**/*",
    "dist/**/*",
    "node_modules",
    "./node_modules",
    "./node_modules/*"
  ]
}
